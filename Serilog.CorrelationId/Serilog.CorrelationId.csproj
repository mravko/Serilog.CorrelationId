<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>
      Serilog.CorrelationId
    </PackageId>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.5</Version>
    <Authors>Marko Vuckovik (mravko)</Authors>
    <Title>
      Serilog.CorrelationId
    </Title>
    <PackageDescription>
      Adds X-Correlation-ID value in Serrilog context either from headers (X-Correlation-ID) or generates new GUID.
    </PackageDescription>
    <RepositoryUrl>https://github.com/mravko/Serilog.CorrelationId</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.1.0" />
    <PackageReference Include="Serilog" Version="2.7.1" />
  </ItemGroup>

</Project>
